# Private key used to sign deployments (0x-prefixed)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Polygon Amoy (PoS testnet) RPC URL
# Get an endpoint from Alchemy/Infura/Polygon or use the public RPC
POLYGON_AMOY_RPC_URL=https://rpc-amoy.polygon.technology

# Polygonscan API key for contract verification on Amoy
# Get yours at: https://amoy.polygonscan.com/apis
POLYGONSCAN_API_KEY=YOUR_POLYGONSCAN_API_KEY

# Optional: Polygon zkEVM Cardona Testnet RPC URL (alternative network)
POLYGON_ZKEVM_RPC_URL=https://rpc.cardona.zkevm-rpc.com

# Optional: Polygonscan API key for zkEVM Cardona
# Get yours at: https://cardona-zkevm.polygonscan.com/apis
POLYGON_ZKEVM_API_KEY=YOUR_CARDONA_API_KEY

# Optional: Enable gas reporter in tests
REPORT_GAS=true

# Optional: CoinMarketCap API key used by gas reporter
COINMARKETCAP_API_KEY=YOUR_CMC_API_KEY